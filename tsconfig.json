{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "jsx": "react-jsx",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist",
        "baseUrl": "./src",
        "paths": {
            "@utils/*": ["utils/*"],
            "@styles/*": ["styles/*"],
            "@components/*": ["components/*"],
            "@jotai/*": ["jotai/*"],
            "@assets/*": ["assets/*"],
            "@icons/*": ["assets/icons/*"]


        },
    },
    "include": ["src/**/*", "src/components/chatPage/.tsx"],
    "exclude": ["node_modules", "**/*.spec.ts"]
}
